{
  "name": "bytes-react-ui",
  "version": "0.0.1",
  "private": true,
  "author": "bytes-sixth(https://github.com/bytes-sixth)",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --check \"src/**/*.{js,jsx}\" && eslint \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\" && eslint --fix \"src/**/*.{js,jsx}\"",
    "test": "yarn run jest -env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bytes-sixth/bytes-react-cli.git",
    "directory": "packages/bytes-react-ui"
  },
  "homepage": "https://github.com/bytes-sixth/bytes-react-cli",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/preset-env": "^7.15.6",
    "@testing-library/react": "^12.1.0",
    "antd": "^4.16.13",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@testing-library/dom": "^8.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.2.1",
    "@vitejs/plugin-react": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.2",
    "babel-preset-react-app": "^10.0.0",
    "carlo": "^0.9.46",
    "detect-installer": "^1.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.2",
    "jest-circus": "^27.2.2",
    "jest-less-loader": "^0.1.1",
    "jest-scss-transform": "^1.0.1",
    "jest-watch-typeahead": "^0.6.4",
    "koa": "^2.13.3",
    "less": "^4.1.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.4.1",
    "puppeteer": "^10.4.0",
    "vite": "^2.5.4",
    "vite-plugin-imp": "^2.0.9",
    "vite-plugin-mock": "^2.9.6"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/mocks/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.scss$": "jest-scss-transform",
      "^.+\\.css$": "<rootDir>/jest/mocks/cssMock.js",
      "^.+\\.(less)$": "jest-less-loader"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  }
}
